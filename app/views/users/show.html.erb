<%= render :partial => "user_row", :locals =>{:user =>@user, :query => ""}  %>
<script type="text/javascript" >
    msg = "<%=flash_msg @msg %>" ;
    activeRow = 0;
    $("#a_<%=  @user.id%>").click(function(e){
      
      var url = $(this).attr("data-url");
      var id  = $(this).attr("data-rel");
      if(activeRow){
          alert("Please complete the current editing row , then start new editing");
          return false;
      }
      getEditForm(url,id);
      return false;
    });
</script>
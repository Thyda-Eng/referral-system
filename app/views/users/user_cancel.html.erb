<%= render :partial => "user_row", :locals =>{:user =>@user}  %>
<script type="text/javascript" >
    msg = "<%=flash_msg flash %>" ;
    $("#a_<%=  @user.id%>").click(function(e){
      var url = $(this).attr("data-url");
      var id  = $(this).attr("data-rel");
      $.ajax({
          url: url,
          method: "get",
          success: function(responseText, status, responseObj){
             $("#tr_"+ id ).html(responseText);
             
          },
          complete: function(){
            showNotice();
          }
      });

      e.preventDefault();
      return false;
    });
</script>
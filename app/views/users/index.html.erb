<% content_for :head do %>
    <%= javascript_include_tag 'jquery.validate.min' %>
    <%= javascript_include_tag 'users/index' %>
<% end %>
<h2 style="float:left;" >List of users</h2>

<div class="clear"></div>
<h4 class="round msg-error" id="actionNotifier" style="display: none;" > </h4>
<%= will_paginate @users %>

<div style="float:right;" > <%= link_to "Import", import_form_users_path %> </div>
<div class="clear"> </div>
<%= render :partial => "list_users", :locals => { :users => @users, :user => @user} %>

<div id="first_row">
  <span id="breadcrumb">
    You are here:
    <% @breadcrumb.each_with_index do |crumb, i| %>
      <%= ' > ' if i > 0 %>
      <% if crumb[:place] %>
        <% if crumb[:place] == 0 %>
          <%= link_to crumb[:label], :action => :index %>
        <% else %>
          <%= link_to crumb[:label], "?place=#{crumb[:place].id}" %>
        <% end %>
      <% else %>
        <b><%= crumb[:label] %></b>
      <% end %>
    <% end %>
  </span>
  <div id="tabs">
    <ul>
      <li>Map</li>
      <li class="selected">All messages</li>
      <li>Error messages</li>
    </ul>
  </div>
</div>
<div id="second_row">
  <div id="navbar">
    <div id="places">
      <% @places.each do |place| %>
        <%= link_to place.name, "?place=#{place.id}" %><br/>
      <% end %>
    </div>
    <div id="users">
      <div id="users_title">Users</div>
      <ul>
        <li>Foo</li>
        <li>Bar</li>
        <li>Baz</li>
      </ul>
    </div>
  </div>
  <div id="reports">
    <% yield :reports %>
  </div>
</div>

<% content_for :head do %>
  <%= javascript_include_tag 'jquery.fancybox-1.3.4' %>
  <%= stylesheet_link_tag "jquery.fancybox-1.3.4" %>
  <script type="text/javascript" >
    $(function(){
      createFancyBox();
    });

    function createFancyBox(){
      $("a.report_detail").fancybox();
    }

  </script>
<%  end %>
<% content_for :reports do %>

 <div class="round report" >
    <%= will_paginate @reports %>
    <div >
      <div style="float:right;">
        <%= link_to "<span>Export result </span>".html_safe , report_csv_reports_url(params.except(:controller, :action)) , :class =>" round slide_label", :id =>"csv_export_case" %>
      </div>
    </div>

    <div class="clear"> </div>
      <%= render "table_with_cases" if(params[:ncase] == "1")  %>
      <%= render "table_with_no_cases" if(params[:ncase] == "0")  %>
    </div>
<% end %>
